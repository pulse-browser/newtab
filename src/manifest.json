{

    "manifest_version": 2,
    "name": "Focus NewTab",
    "version": "1.0",
  
    "description": "Adds a custom new-tab to focus browser.",
  
    "icons": {
      "48": "icons/focus-48.png"
    },
    "permissions": [
        "topSites",
        "search"
      ],

    "chrome_url_overrides": {
        "newtab": "index.html"
      },
      "chrome_settings_overrides": {
        "homepage": "index.html"
      }
  
  }